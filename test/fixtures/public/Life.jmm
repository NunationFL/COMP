        UNDERPOP_LIM = 2;
        OVERPOP_LIM = 3;
        REPRODUCE_NUM = 3;

        LOOPS_PER_MS = 225000;

        /* Instance variables */
        field = this.field(lineLenA);
        lineLen = lineLenA[0];

        xMax = lineLen - 1;
        yMax = field.length / lineLen - 1;

        return true;
    }

